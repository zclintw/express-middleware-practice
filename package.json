{
  "name": "projectd",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "web": "node ./bin/www",
    "web:dev": "DEBUG=projectd:* nodemon ./bin/www",
    "redis": "docker stop myredis; docker run --rm -p 6379:6379 --name myredis -d redis",
    "redis:stop": "docker rm -f myredis",
    "start": "npm run redis && npm run web",
    "dev": "npm run redis && npm run web:dev",
    "test": "DEBUG=projectd:* jest --coverage __tests__/ratelimit.test.js"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "ioredis": "^4.19.0",
    "ipaddr.js": "^2.0.0",
    "jade": "~1.11.0",
    "moment": "^2.29.1",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "eslint": "^7.12.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-import": "^2.22.1",
    "ioredis-mock": "^5.0.0",
    "jest": "^26.6.1",
    "redis-mock": "^0.52.0"
  }
}
